<?xml version = "1.0" encoding = "UTF-8"?>

<commands xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation = "commands.xsd"
>
    
  <command name = "command">
    <meta aliases = "cmd, comm" description = "command description" usage = "command usage"/>
    <permission permission = "command.permission" message = "You do not have permission to use this command"/>
    
    <commands>
        
      <command name = "subcommand">
        <meta aliases = "subcmd, subcomm" description = "subcommand description" usage = "subcommand usage"/>
        <permission permission = "subcommand.permission" message = "You do not have permission to use this subcommand"/>
      </command>
      
    </commands>
  </command>
  
</commands>